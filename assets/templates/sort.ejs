
<% _.each(sortAndPaginate.attributes, function (attribute) { 
	if (attribute.sortable) { %>
	<th data-field="<%=attribute.name%>">
		<a href="/<%=sortAndPaginate.model%>?sortType=<%=((sortAndPaginate.sortBy == attribute.name) 
			? sortAndPaginate.sortType : 'DESC')%>&sortBy=<%=attribute.name%>
			<%=(req.param('filterIsOpen')?('&filterIsOpen='+req.param('filterIsOpen')):'')%>
			<%=(req.param('nameFilter')?('&nameFilter='+req.param('nameFilter')):'')%>
			<%=(req.param('validity')?('&validity='+req.param('validity')):'')%>">
			<%=i18n(attribute.name)%>
			<i class="material-icons icon-sort">keyboard_arrow_<%=((sortAndPaginate.sortBy == attribute.name) ? (sortAndPaginate.sortType == 'DESC' ?  'down' : 'up') : 'up')%></i>
		</a>
	</th>
<% }else{%>
	<th data-field="<%=attribute.name%>"><%=i18n(attribute.name)%></th>
<% }}) %>