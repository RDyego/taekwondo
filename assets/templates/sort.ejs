
<% _.each(sortAndPaginate.attributes, function (attribute) { 
	if (attribute.sortable) { %>
	<th data-field="<%=attribute.name%>">
		<a href="/<%=sortAndPaginate.model%>?sortType=<%=((sortAndPaginate.sortBy == attribute.name) ? sortAndPaginate.sortType : 'DESC')%>&sortBy=<%=attribute.name%>">
			<%=i18n(attribute.name)%>
			<i class="material-icons">keyboard_arrow_<%=((sortAndPaginate.sortBy == attribute.name) ? (sortAndPaginate.sortType == 'DESC' ?  'down' : 'up') : 'up')%></i>
		</a>
	</th>
<% }else{%>
	<th data-field="<%=attribute.name%>"><%=i18n(attribute.name)%></th>
<% }}) %>