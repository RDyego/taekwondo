<div class="container">
	<div class="center">
		<form action="/athlete/update/<%=athlete.id%>" method="post" class="col s12">
			<div class="row">
				<div class="input-field col s6">
					<input id="name" name="name" type="text" value="<%=(athlete.name)?athlete.name:''%>">
					<label for="name" ><%=i18n('Name')%></label>
				</div>
				<div class="input-field col s6">
					<input id="email" name="email" type="email" value="<%=(athlete.email)?athlete.email:''%>">
					<label for="email"><%=i18n('Email')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s6">
					<input id="fatherName" name="fatherName" type="text" value="<%=(athlete.fatherName)?athlete.fatherName:''%>">
					<label for="fatherName"><%=i18n('Father')%></label>
				</div>
				<div class="input-field col s6">
					<input id="motherName" name="motherName" type="text" value="<%=(athlete.motherName)?athlete.motherName:''%>">
					<label for="motherName"><%=i18n('Mother')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s3">
					<input id="bday" name="bday" type="date" class="datepicker" value="<%=(athlete.bday)?athlete.bday:''%>">
					<label for="bday"><%=i18n('Date of Birth')%></label>
				</div>
				<div class="input-field col s3">
					<select id="education" name="education">
						<%_.each(Athlete.attributes.education.enum, function (enumValue) {%>
							<option <%=(athlete.education == enumValue)?'selected':''%> value="<%=enumValue%>"><%=i18n(enumValue)%></option>
						<%})%>
					</select>
					<label for="education"><%=i18n('Education')%></label>
				</div>
				<div class="input-field col s3">
					<input id="naturalness" name="naturalness" type="text" value="<%=(athlete.naturalness)?athlete.naturalness:''%>">
					<label for="naturalness"><%=i18n('Naturalness')%></label>
				</div>
				<div class="input-field col s3">
					<input id="nationality" name="nationality" type="text" value="<%=(athlete.nationality)?athlete.nationality:''%>">
					<label for="nationality"><%=i18n('Nationality')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s6">
					<input id="profession" name="profession" type="text" value="<%=(athlete.profession)?athlete.profession:''%>">
					<label for="profession"><%=i18n('Profession')%></label>
				</div>
				<div class="input-field col s3">
					<input id="rg" name="rg" type="text" value="<%=(athlete.rg)?athlete.rg:''%>">
					<label for="rg"><%=i18n('RG')%></label>
				</div>
				<div class="input-field col s3">
					<input id="cpf" name="cpf" type="text" value="<%=(athlete.cpf)?athlete.cpf:''%>">
					<label for="cpf"><%=i18n('CPF')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s9">
					<input id="address" name="address" type="text" value="<%=(athlete.address)?athlete.address:''%>">
					<label for="address"><%=i18n('Address')%></label>
				</div>
				<div class="input-field col s3">
					<input id="cep" name="cep" type="text" value="<%=(athlete.cep)?athlete.cep:''%>">
					<label for="cep"><%=i18n('CEP')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s9">
					<input id="businessAddress" name="businessAddress" type="text" value="<%=(athlete.businessAddress)?athlete.businessAddress:''%>">
					<label for="businessAddress"><%=i18n('Business Address')%></label>
				</div>
				<div class="input-field col s3">
					<input id="phone" name="phone" type="text" value="<%=(athlete.phone)?athlete.phone:''%>">
					<label for="phone"><%=i18n('Phone')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s3">
					<select id="graduation" name="graduation">
						<%_.each(Athlete.attributes.graduation.enum, function (enumValue) {%>
							<option <%=(athlete.graduation == enumValue)?'selected':''%> value="<%=enumValue%>"><%=i18n(enumValue)%></option>
						<%})%>
					</select>
					<label for="graduation"><%=i18n('Graduation')%></label>
				</div>
				<div class="input-field col s3">
					<input id="dateStarted" name="dateStarted" type="date" class="datepicker" value="<%=(athlete.dateStarted)?athlete.dateStarted:''%>">
					<label for="dateStarted"><%=i18n('Date Started')%></label>
				</div>
				<div class="input-field col s3">
					<select id="bloodGroup" name="bloodGroup">
						<%_.each(Athlete.attributes.bloodGroup.enum, function (enumValue) {%>
							<option <%=(athlete.bloodGroup == enumValue)?'selected':''%> value="<%=enumValue%>"><%=i18n(enumValue).toUpperCase()%></option>
						<%})%>
					</select>
					<label for="bloodGroup"><%=i18n('Blood Group')%></label>
				</div>
				<div class="input-field col s3">
					<select id="rhFactor" name="rhFactor">
						<%_.each(Athlete.attributes.rhFactor.enum, function (enumValue) {%>
							<option <%=(athlete.rhFactor == enumValue)?'selected':''%> value="<%=enumValue%>"><%=i18n(enumValue)%></option>
						<%})%>
					</select>
					<label for="rhFactor"><%=i18n('RH Factor')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s6">
					<input id="instructorName" name="instructorName" type="text" value="<%=(athlete.instructorName)?athlete.instructorName:''%>">
					<label for="instructorName"><%=i18n('Instructor Name')%></label>
				</div>
				<div class="input-field col s6">
					<input id="associationName" name="associationName" type="text" value="<%=(athlete.associationName)?athlete.associationName:''%>">
					<label for="associationName"><%=i18n('Association Name')%></label>
				</div>
			</div>
			<div class="row">
				<div class="col s6">
					<div class="file-field input-field">
						<input class="file-path validate" type="text"/>
						<div class="btn">
							<span><%=i18n('Photo')%></span>
							<input type="file" id="photo" name="photo"/>
						</div>
				    </div>
			    </div>
	    	</div>
			<div class="row">
				<button class="btn waves-effect waves-light" type="submit" name="action">Edit
					<i class="material-icons">send</i>
				</button>
			</div>
			<input type="hidden" name="_csrf" value="<%=_csrf%>">
		</form>
	</div>
</div>