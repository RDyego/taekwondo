<div class="container">
	<div class="center">
		<form action="/athlete/update/<%=athlete.id%>" enctype="multipart/form-data" method="post" class="col s12">
			<div class="row">
				<div class="input-field col s6">
					<input id="name" name="name" type="text" maxlength="50" value="<%=(athlete.name)?athlete.name:''%>">
					<label for="name" ><%=i18n('Name')%></label>
				</div>
				<div class="input-field col s6">
					<input id="email" name="email" type="email" maxlength="50" value="<%=(athlete.email)?athlete.email:''%>">
					<label for="email"><%=i18n('Email')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s6">
					<input id="fatherName" name="fatherName" type="text" maxlength="50" value="<%=(athlete.fatherName)?athlete.fatherName:''%>">
					<label for="fatherName"><%=i18n('Father')%></label>
				</div>
				<div class="input-field col s6">
					<input id="motherName" name="motherName" type="text" maxlength="50" value="<%=(athlete.motherName)?athlete.motherName:''%>">
					<label for="motherName"><%=i18n('Mother')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s3">
					<input id="bday" name="bday" type="date" class="datepicker" maxlength="20" 
						value="<%=(athlete.bday) ? moment(athlete.bday).format('DD/MM/YYYY') : ''%>">
					<label for="bday"><%=i18n('Date of Birth')%></label>
				</div>
				<div class="input-field col s3">
					<select id="education" name="education">
						<%_.each(Athlete.attributes.education.enum, function (enumValue) {%>
							<option <%=(athlete.education == enumValue)?'selected':''%> value="<%=enumValue%>"><%=i18n(enumValue)%></option>
						<%})%>
					</select>
					<label for="education"><%=i18n('Education')%></label>
				</div>
				<div class="input-field col s3">
					<input id="naturalness" name="naturalness" type="text" maxlength="27" value="<%=(athlete.naturalness)?athlete.naturalness:''%>">
					<label for="naturalness"><%=i18n('Naturalness')%></label>
				</div>
				<div class="input-field col s3">
					<input id="nationality" name="nationality" type="text" maxlength="27" value="<%=(athlete.nationality)?athlete.nationality:''%>">
					<label for="nationality"><%=i18n('Nationality')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s6">
					<input id="profession" name="profession" type="text" maxlength="50" value="<%=(athlete.profession)?athlete.profession:''%>">
					<label for="profession"><%=i18n('Profession')%></label>
				</div>
				<div class="input-field col s3">
					<input id="rg" name="rg" type="text" maxlength="27" value="<%=(athlete.rg)?athlete.rg:''%>">
					<label for="rg"><%=i18n('RG')%></label>
				</div>
				<div class="input-field col s3">
					<input id="cpf" name="cpf" type="text" maxlength="27" value="<%=(athlete.cpf)?athlete.cpf:''%>">
					<label for="cpf"><%=i18n('CPF')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s9">
					<input id="address" name="address" maxlength="90" type="text" value="<%=(athlete.address)?athlete.address:''%>">
					<label for="address"><%=i18n('Address')%></label>
				</div>
				<div class="input-field col s3">
					<input id="cep" name="cep" type="text" maxlength="27" value="<%=(athlete.cep)?athlete.cep:''%>">
					<label for="cep"><%=i18n('CEP')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s9">
					<input id="businessAddress" name="businessAddress" type="text" maxlength="90" value="<%=(athlete.businessAddress)?athlete.businessAddress:''%>">
					<label for="businessAddress"><%=i18n('Business Address')%></label>
				</div>
				<div class="input-field col s3">
					<input id="phone" name="phone" type="text" maxlength="27" value="<%=(athlete.phone)?athlete.phone:''%>">
					<label for="phone"><%=i18n('Phone')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s3">
					<select id="graduation" name="graduation">
						<%_.each(Athlete.attributes.graduation.enum, function (enumValue) {%>
							<option <%=(athlete.graduation == enumValue)?'selected':''%> value="<%=enumValue%>"><%=i18n(enumValue)%></option>
						<%})%>
					</select>
					<label for="graduation"><%=i18n('Graduation')%></label>
				</div>
				<div class="input-field col s3">
					<input id="dateStarted" name="dateStarted" type="date" class="datepicker" maxlength="20" 
                        value="<%=(athlete.dateStarted)?moment(athlete.dateStarted).format('DD/MM/YYYY'):''%>">
					<label for="dateStarted"><%=i18n('Date Started')%></label>
				</div>
				<div class="input-field col s3">
					<select id="bloodGroup" name="bloodGroup">
						<%_.each(Athlete.attributes.bloodGroup.enum, function (enumValue) {%>
							<option <%=(athlete.bloodGroup == enumValue)?'selected':''%> value="<%=enumValue%>"><%=i18n(enumValue).toUpperCase()%></option>
						<%})%>
					</select>
					<label for="bloodGroup"><%=i18n('Blood Group')%></label>
				</div>
				<div class="input-field col s3">
					<select id="rhFactor" name="rhFactor">
						<%_.each(Athlete.attributes.rhFactor.enum, function (enumValue) {%>
							<option <%=(athlete.rhFactor == enumValue)?'selected':''%> value="<%=enumValue%>"><%=i18n(enumValue)%></option>
						<%})%>
					</select>
					<label for="rhFactor"><%=i18n('RH Factor')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s6">
					<input id="instructorName" name="instructorName" type="text" maxlength="50" value="<%=(athlete.instructorName)?athlete.instructorName:''%>">
					<label for="instructorName"><%=i18n('Instructor Name')%></label>
				</div>
				<div class="input-field col s6">
					<input id="associationName" name="associationName" type="text" maxlength="50" value="<%=(athlete.associationName)?athlete.associationName:''%>">
					<label for="associationName"><%=i18n('Association Name')%></label>
				</div>
			</div>
            <input type="hidden" name="_csrf" value="<%=_csrf%>">
			<div class="row">
				<div class="col s6">
					<div class="file-field input-field">
						<div class="blue btn">
							<span><%=i18n('Photo')%></span>
							<input type="file" id="photo" name="photo"/>
						</div>
						<div class="file-path-wrapper">
							<input class="file-path validate" type="text">
						</div>
					</div>
				</div>
                <div class="input-field col s3">
					<input id="validityFrom" name="validityFrom" type="date" class="datepicker" maxlength="20" 
                        value="<%=(athlete.validityFrom)?moment(athlete.validityFrom).format('DD/MM/YYYY'):((athlete.validity)?moment(athlete.validity).add(-1, 'years').format('DD/MM/YYYY'):moment().format('DD/MM/YYYY'))%>">
					<label for="validityFrom"><%=i18n('Validity From')%></label>
				</div>
                <div class="input-field col s3">
					<input id="validityTo" name="validityTo" type="date" class="datepicker" maxlength="20" 
                        value="<%=(athlete.validityTo)?moment(athlete.validityTo).format('DD/MM/YYYY'):((athlete.validity)?moment(athlete.validity).format('DD/MM/YYYY'):moment().add(1, 'years').format('DD/MM/YYYY'))%>">
					<label for="validityTo"><%=i18n('Validity To')%></label>
				</div>
			</div>
			<div class="row">
				<button class="blue btn waves-effect waves-light" type="submit" name="action"><%=i18n('Edit')%>
					<i class="material-icons right">send</i>
				</button>
			</div>
			<input type="hidden" name="validity" 
				value="<%=(athlete.validity)?moment(athlete.validity).format('DD/MM/YYYY'):moment().add(1, 'years').format('DD/MM/YYYY')%>">
		</form>
	</div>
</div>