<div class="container">
	<div class="center">
		<form class="col s12">
			<div class="row">
				<div class="input-field col s6">
					<input id="name" name="name" type="text" disabled value="<%=athlete.name%>">
					<label for="name" ><%=i18n('Name')%></label>
				</div>
				<div class="input-field col s6">
					<input id="email" name="email" type="email" disabled value="<%=athlete.email%>">
					<label for="email"><%=i18n('Email')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s6">
					<input id="fatherName" name="fatherName" type="text" disabled>
					<label for="fatherName"><%=i18n('Father')%></label>
				</div>
				<div class="input-field col s6">
					<input id="motherName" name="motherName" type="text" disabled>
					<label for="motherName"><%=i18n('Mother')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s3">
					<input id="bday" name="bday" type="text" disabled>
					<label for="bday"><%=i18n('Date of Birth')%></label>
				</div>
				<div class="input-field col s3">
					<select id="education" name="education" disabled>
						<%_.each(Athlete.attributes.education.enum, function (eduEnum) {%>
							<option value="<%=eduEnum%>"><%=i18n(eduEnum)%></option>
						<%})%>
					</select>
					<label for="education"><%=i18n('Education')%></label>
				</div>
				<div class="input-field col s3">
					<input id="naturalness" name="naturalness" type="text" disabled>
					<label for="naturalness"><%=i18n('Naturalness')%></label>
				</div>
				<div class="input-field col s3">
					<input id="nationality" name="nationality" type="text" disabled>
					<label for="nationality"><%=i18n('Nationality')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s6">
					<input id="profession" name="profession" type="text" disabled>
					<label for="profession"><%=i18n('Profession')%></label>
				</div>
				<div class="input-field col s3">
					<input id="rg" name="rg" type="text" disabled>
					<label for="rg"><%=i18n('RG')%></label>
				</div>
				<div class="input-field col s3">
					<input id="cpf" name="cpf" type="text" disabled>
					<label for="cpf"><%=i18n('CPF')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s9">
					<input id="address" name="address" type="text" disabled>
					<label for="address"><%=i18n('Address')%></label>
				</div>
				<div class="input-field col s3">
					<input id="cep" name="cep" type="text" disabled>
					<label for="cep"><%=i18n('CEP')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s9">
					<input id="businessAddress" name="businessAddress" type="text" disabled>
					<label for="businessAddress"><%=i18n('Business Address')%></label>
				</div>
				<div class="input-field col s3">
					<input id="phone" name="phone" type="text" disabled>
					<label for="phone"><%=i18n('Phone')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s3">
					<select id="graduation" name="graduation" disabled>
						<%_.each(Athlete.attributes.graduation.enum, function (enumValue) {%>
							<option value="<%=enumValue%>"><%=i18n(enumValue)%></option>
						<%})%>
					</select>
					<label for="graduation"><%=i18n('Graduation')%></label>
				</div>
				<div class="input-field col s3">
					<input id="dateStarted" name="dateStarted" type="text" disabled>
					<label for="dateStarted"><%=i18n('Date Started')%></label>
				</div>
				<div class="input-field col s3">
					<input id="bloodGroup" name="bloodGroup" type="text" disabled>
					<label for="bloodGroup"><%=i18n('Blood Group')%></label>
				</div>
				<div class="input-field col s3">
					<select id="rhFactor" name="rhFactor" disabled>
						<%_.each(Athlete.attributes.rhFactor.enum, function (enumValue) {%>
							<option value="<%=enumValue%>"><%=i18n(enumValue)%></option>
						<%})%>
					</select>
					<label for="rhFactor"><%=i18n('RH Factor')%></label>
				</div>
			</div>
			<div class="row">
				<div class="input-field col s6">
					<input id="instructorName" name="instructorName" type="text" disabled>
					<label for="instructorName"><%=i18n('Instructor Name')%></label>
				</div>
				<div class="input-field col s6">
					<input id="associationName" name="associationName" type="email" disabled>
					<label for="associationName"><%=i18n('Association Name')%></label>
				</div>
			</div>
			<div class="row">
				<div class="col s6">
					<div class="file-field input-field">
						<input class="file-path validate" type="text" disabled/>
						<div class="btn">
							<span>Photo</span>
							<input type="file" id="photo" name="photo" disabled/>
						</div>
				    </div>
			    </div>
	    	</div>
		</form>
	</div>
</div>